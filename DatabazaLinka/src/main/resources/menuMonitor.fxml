                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="loginButton" layoutX="207.0" layoutY="238.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="227.0" text="Zmena prihlásenej osoby">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="exitButton" layoutX="601.0" layoutY="10.0" mnemonicParsing="false" text="X" />
            <Text layoutX="14.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Monitor">
