  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="startButton" layoutX="209.0" layoutY="126.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="227.0" text="Zapnúť program">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="editButton" layoutX="207.0" layoutY="183.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="227.0" text="Vykonať zmeny">
